<?xml version="1.0" encoding="UTF-8"?>

<fileset-config file-format-version="1.2.0" simple-config="true" sync-formatter="false">
  <local-check-config name="Spi4j_CheckStyle" location="conf/Checks_Spi4j.xml" type="project" description="">
    <additional-data name="protect-config-file" value="false"/>
  </local-check-config>
  <fileset name="all" enabled="true" check-config-name="Spi4j_CheckStyle" local="true">
    <file-match-pattern match-pattern="." include-pattern="true"/>
    <file-match-pattern match-pattern=".*png" include-pattern="false"/>
    <file-match-pattern match-pattern=".*gif" include-pattern="false"/>
  </fileset>
  <filter name="NonSrcDirs" enabled="true"/>
</fileset-config>
